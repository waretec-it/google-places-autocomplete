<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="WareTec" constructor="GooglePlacesAutocomplete" version="1.0.0" display-name-key="Google Places Autocomplete" description-key="Address autocompletion with Google Places API" control-type="standard" >
    <external-service-usage enabled="false">
    </external-service-usage>
    <property name="googleapikey" display-name-key="Google Api Key" description-key="Enter a valid Google Api Key" of-type="SingleLine.Text" usage="input" required="true" />
    <property name="placeholder" display-name-key="Placeholder" description-key="Placeholder" of-type="SingleLine.Text" usage="input" required="false" />
    <property name="street" display-name-key="Street" description-key="Street" of-type="SingleLine.Text" usage="bound" required="true" />
    <property name="city" display-name-key="City" description-key="City" of-type="SingleLine.Text" usage="bound" required="false" />
    <property name="state" display-name-key="State" description-key="State" of-type="SingleLine.Text" usage="bound" required="false" />
    <property name="zipcode" display-name-key="Zipcode" description-key="Zipcoce" of-type="SingleLine.Text" usage="bound" required="false" />
    <property name="country" display-name-key="Country" description-key="Country" of-type="SingleLine.Text" usage="bound" required="false" />
    <resources>
      <code path="index.ts" order="1"/>
      <css path="css/googleplacesautocomplete.css" order="1"/>
    </resources>
  </control>
</manifest>
